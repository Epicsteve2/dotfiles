{
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.fontFamily": "'FiraCode Nerd Font', 'MesloLGS NF', 'Monofur Nerd Font'",
  "editor.fontLigatures": true,
  "editor.minimap.renderCharacters": false,
  "editor.renderWhitespace": "boundary",
  "editor.scrollBeyondLastLine": false,
  "editor.smoothScrolling": true,
  "editor.tabCompletion": "on",
  "editor.trimAutoWhitespace": true,
  "explorer.openEditors.visible": 0,
  "explorer.sortOrder": "modified",
  "files.autoSave": "onFocusChange",
  "files.trimTrailingWhitespace": true,
  "search.showLineNumbers": true,
  "search.smartCase": true,
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.enabled": false,
  "security.workspace.trust.startupPrompt": "never",
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.fontFamily": "'FiraCode Nerd Font', 'MesloLGS Nerd Font', 'Monofur Nerd Font'",
  "terminal.integrated.fontSize": 14,
  "update.mode": "none",
  "window.newWindowDimensions": "maximized",
  "workbench.colorTheme": "Monokai",
  "workbench.editor.enablePreview": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.list.smoothScrolling": true,
  "workbench.startupEditor": "newUntitledFile",
  "editor.minimap.enabled": false,
  "path-autocomplete.triggerOutsideStrings": true,
  "html.suggest.html5": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "files.associations": {
    ".env*": "dotenv"
  },
  "pasteImage.path": "${currentFileDir}/images/",
  "dashboard.projectData": null,
  "latex-workshop.view.pdf.viewer": "tab",
  "editor.linkedEditing": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.sideBar.location": "right",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "haskell.manageHLS": "PATH",
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "markdown.experimental.validate.enabled": true,
  "svelte.enable-ts-plugin": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.stickyScroll.enabled": true,
  "[java]": {
    "editor.defaultFormatter": "redhat.java"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "python.formatting.provider": "black",
  "latex-workshop.latex.autoBuild.run": "onSave",
  // "git.mergeEditor": true,
  "background.enabled": true,
  "background.loop": false,
  "background.useDefault": false,
  "background.useFront": false,
  "background.style": {
    "content": "''",
    "pointer-events": "none",
    "position": "absolute",
    "z-index": "99999",
    "width": "100%",
    "height": "100%",
    "background-size": "cover",
    "background-repeat": "no-repeat",
    "opacity": 0.2
  },
  "background.customImages": [
    "/home/stephen/Pictures/Wallpapers/wp8281628-suisei-hoshimachi-wallpapers.jpg",
  ]
}